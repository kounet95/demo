type Query{
    compteslistes: [BanqueTest],
    comptelist (id : String) : BanqueTest
}
type BanqueTest{
    id: String,
    createdAt: Float,
    balance: Float,
    currency: String,
    type:String
}
type Mutation{
    addcompte(banqueDTO: BanqueDTO): BanqueTest
    update(id:String, banqueDTO: BanqueDTO) : BanqueTest
}
input BanqueDTO{
    createdAt: Float,
    balance: Float,
    currency: String,
    type:String
}